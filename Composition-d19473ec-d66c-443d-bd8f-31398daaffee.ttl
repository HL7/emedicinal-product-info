     fhir:index 6;
     fhir:Composition.section.title [ fhir:value "9.5 妊婦" ];
     fhir:Composition.section.code [
       fhir:CodeableConcept.coding [
         fhir:index 0;
         fhir:Coding.system [ fhir:value "https://pmda.jp/" ];
         fhir:Coding.code [ fhir:value "9" ]
